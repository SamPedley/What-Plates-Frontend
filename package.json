{
  "name": "what-plates",
  "version": "1.0.0",
  "description": "Math is for losers.  Let us figure out how many plates you need.  All the gains with none of the math.",
  "repository": {
    "type": "git",
    "url": "https://github.com/SamPedley/What-Plates"
  },
  "author": "Sam Pedley",
  "license": "MIT",
  "scripts": {
    "start": "node ./browserSync.js --development",
    "prod": "webpack --config ./webpack.config.js -p",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "lodash": "^4.11.1",
    "material-ui": "^0.15.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.8.0",
    "browser-sync": "^2.8.0",
    "css-loader": "^0.25.0",
    "jest-cli": "^15.1.1",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.10.5",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/app/src/js"
    ],
    "testDirectoryName": "tests",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  }
}
